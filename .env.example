# Supabase Configuration
VITE_SUPABASE_PROJECT_ID=your_project_id
VITE_SUPABASE_PUBLISHABLE_KEY=your_publishable_key  
VITE_SUPABASE_URL=https://your_project_id.supabase.co

# Feature Flags
# Enable/disable features in production
# Set to 'true' or '1' to enable, 'false' or '0' to disable
# Default: false (disabled in production)

# Payroll A07 feature - includes A07 upload, analysis, and reconciliation
VITE_FEATURE_PAYROLL_A07=false

# Report Builder feature - includes dashboard widgets and custom reports
VITE_FEATURE_REPORT_BUILDER=false  

# Advanced AI features - includes AI Revy, multi-agent studio, advanced analytics
VITE_FEATURE_ADVANCED_AI=false

# Internal Testing Examples:
# VITE_FEATURE_PAYROLL_A07=true
# VITE_FEATURE_REPORT_BUILDER=true
# VITE_FEATURE_ADVANCED_AI=true